{
  "extends": "expo/tsconfig.base",
  
  "compilerOptions": {
    "noImplicitAny":false,
    "typeRoots": ["./node_modules/@types", "./.expo/types"],
    "strict": true,
    "paths": {
      "@/*": [
        "./*"
      ]
    }
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    ".expo/types/**/*.ts",
    "expo-env.d.ts"
  ],
  "exclude":[
    "node_modules"
  ]
}